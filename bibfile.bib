@article{Box-Steffensmeier2018,
abstract = {Interest groups coordinate to achieve political goals. However, these groups are heterogeneous, and the division of labor within these coalitions varies. We explore the presence of distinct roles in coalitions of environmental interest groups, and analyse which factors predict if an organization takes on a particular role. To model these latent dynamics, we introduce the ego-ERGM. We find that a group's budget, member size, staff size, and degree centrality are influential in distinguishing between three role assignments. These results provide insight into the roles adopted in carrying out coalition tasks. This approach shows promise for understanding a host of networks.},
author = {Box-Steffensmeier, J M and Campbell, B W and Christenson, D P and Navabi, Z},
